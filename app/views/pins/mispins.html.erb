<p id="notice"><%= notice %></p>

<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
      <div class="box">
        <div class="panel panel-default">
          <%= link_to image_tag(pin.image.url(:medium)), pin %><br/>
          <div class="panel-body">
            <%= pin.description %><br/>
          </div>

        </div>


      </div>
  <% end %>
</div>

